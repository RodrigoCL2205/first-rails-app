<!DOCTYPE html>
<html>
  <head>
    <title>FirstRailsApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div style="background: violet; padding: 16px">This is navbar
    <p><%= link_to "Know more about us", about_path %></p>
    <p><%= link_to "contact us", contact_path %></p>
    <form action="/contact" method="get">
      <input type="text" name="teacher" value="<%= params[:teacher] %>"  placeholder="Who are you looking for?">
      <input type="text" name="message">
      <input type="submit">
    </form>
  </div>
    <%= yield %>
  </body>
</html>
